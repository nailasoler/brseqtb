process {
    executor = 'local'
    shell = ['/bin/bash', '-euo', 'pipefail']
}

workDir = 'work'

trace {
    enabled = true
    file = 'logs/trace.txt'
}

timeline {
    enabled = true
    file = 'logs/timeline.html'
}

report {
    enabled = true
    file = 'logs/report.html'
}

dag {
    enabled = true
    file = 'logs/dag.html'
}

